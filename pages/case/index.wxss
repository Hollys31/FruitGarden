/* pages/case/index.wxss */

page {
  background: #ffb15d;
}

swiper {
  width: 100vw;
  height: 100vh;
}

.index {
  position: relative;
}

.index .bg {
  width: 100vw;
  height: 66vh;
}

.index .fruit {
  position: absolute;
  top: 430rpx;
  left: 80rpx;
  height: 631rpx;
  width: 626rpx;
  z-index: 2;
}

/* @media screen and (min-height: 750px) {
