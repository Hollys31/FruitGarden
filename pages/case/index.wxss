.index .left {
  position: absolute;
  left: -200rpx;
  top: -85rpx;
  width: 289rpx;
  height: 1116rpx;
  z-index: 1;
}

.active .index .left-ani {
  animation: mover 1s linear forwards;
}

@keyframes mover {
  from {
    transform: translateX(0rpx);
  }

  to {
    transform: translateX(170rpx);
  }
}

.index .right {
  position: absolute;
  right: -200rpx;
  top: -50rpx;
  width: 598rpx;
  height: 929rpx;
  z-index: 1;
}

.active .index .right-ani {
  animation: movel 1s linear forwards;
}

@keyframes movel {
  from {
    transform: translateX(0rpx);
  }

  to {
    transform: translateX(-160rpx);
  }
}

